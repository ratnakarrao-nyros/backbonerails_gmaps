<h1>New Visitor</h1>

<form id="new-visitor" name="visitor">
  <div class="field">
    <label for="name"> Name:</label>
    <input type="text" name="name" id="name" value="<%= name %>" >
  </div>

  <div class="field">
    <label for="start"> Start:</label>
    <input type="text" name="start" id="start" value="<%= start %>" >
  </div>

  <div class="field">
    <label for="end"> End:</label>
    <input type="text" name="end" id="end" value="<%= end %>" >
  </div>


  <div class="field">
    <label for="place"> Your Current City:</label>
    <input type="text" name="place" id="place" value="<%= place %>" >
  </div>


  <div class="actions">
    <input type="submit" value="Create Visitor" />
  </div>

</form>

<a href="#/index">Back</a>



